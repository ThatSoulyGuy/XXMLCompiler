Module SimpleTest Provides
[
    Uses [
        Language::Core
        Language::System
    ]

    Executable EntryPoint <main> Returns Integer^ Parameters () ->
    {
        Instantiate Integer^ As <num1> = Integer::Constructor(10);
        Instantiate Integer^ As <num2> = Integer::Constructor(20);
        Instantiate Integer^ As <sum> = num1.add(num2);

        Instantiate String^ As <sumStr> = sum.toString();
        Run Console::printLine(sumStr);

        Return Integer::Constructor(0);
    }
]
